{
  "name": "@qnighy/marshal",
  "description": "Decoder for Ruby's Marshal",
  "version": "0.1.3",
  "license": "MIT",
  "homepage": "https://github.com/qnighy/marshal-js",
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "node": "./dist/index.js",
    "default": "./dist/esm/index.js"
  },
  "files": [
    "dist/**",
    "src/error.ts",
    "src/index.ts",
    "src/marshal-object.ts",
    "src/parse.ts"
  ],
  "scripts": {
    "build": "$npm_execpath run build:babel && $npm_execpath run build:babel:esm && $npm_execpath run build:tsc",
    "build:babel": "babel src --out-dir dist --extensions \".ts\" --ignore \"src/**/*.test.ts\"",
    "build:babel:esm": "babel src --out-dir dist/esm --extensions \".ts\" --ignore \"src/**/*.test.ts\" --config-file ./babel.config.esm.js",
    "build:tsc": "tsc -p tsconfig.build.json",
    "fmt": "$npm_execpath run fmt:runprettier -w",
    "fmt:check": "$npm_execpath run fmt:runprettier -c",
    "fmt:runprettier": "prettier src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "$npm_execpath run lint --fix",
    "prepack": "$npm_execpath run build",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@jest/globals": "^27.0.6",
    "@types/node": "^16.4.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "browserslist": [
    "defaults"
  ]
}
